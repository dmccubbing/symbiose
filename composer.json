{
	"name": "symbiose/symbiose",
	"description":"Symbiose, the free and flexible webos.",
	"keywords":["webos","web desktop"],
	"homepage":"http://symbiose.fr.cr",
	"version":"1.0-beta4",
	"license":"MIT",
	"type":"application",
	"authors":[
		{
			"name":"emersion",
			"homepage":"http://emersion.fr"
		}
	],
	"config":{
		"vendor-dir":"lib/vendor"
	},
	"require": {
		"php": ">=5.3.0",
		"ext-json": "*",
		"ext-memcache": "*",
		"cboden/Ratchet": "0.3.*",
		"symbiose/websocket-client": "dev-master",
		"guzzle/http": "~3.6",
		"nitra/php-min": "dev-master",
		"ircmaxell/password-compat": "dev-master",
		"evenement/evenement": "1.0.*",
		"fsi/acl": "0.9.*",
		"gitonomy/gitlib": "dev-master",
		"emersion/php-wrappers": "dev-master"
	},
	"suggest": {
		"ext-ftp": "To mount FTP volumes",
		"ext-ldap": "For LDAP authentication",
		"ext-zip": "For ZIP support"
	},
	"autoload": {
		"psr-0": {"JSMinPlus": "lib/vendor/nitra/php-min"}
	}
}
